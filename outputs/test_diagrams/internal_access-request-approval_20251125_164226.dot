digraph ProcessDiagram {
  graph [rankdir=LR, bgcolor=white, pad=0.5, nodesep=0.6, ranksep=1.0, fontname=Helvetica];
  node [fontname=Helvetica, fontsize=11, shape=rect, style="rounded,filled", fillcolor="#E3F2FD", color="#1565C0", penwidth=1.2, width=3.0];
  edge [color="#1565C0", penwidth=1.2, arrowsize=0.8];
  "node_start" [shape=circle, width=0.70, height=0.70, style=filled, fillcolor="#C8E6C9", color="#2E7D32", label="Start"];
  "node_end" [shape=circle, peripheries=2, width=0.70, height=0.70, style=filled, fillcolor="#FFCDD2", color="#C62828", label="End"];
  subgraph cluster_happy {
    style=rounded;
    color="#B0BEC5";
    penwidth=1.0;
    fontname=Helvetica; fontsize=12; label="Happy Path";
    "node_happy_1" [label=<<TABLE BORDER="0" CELLBORDER="0" CELLPADDING="6"><TR><TD ALIGN="left"><B>Happy Path #1</B></TD></TR><TR><TD ALIGN="left">Small Business Admin submits access request</TD></TR></TABLE>>];
  "node_start" -> "node_happy_1";
    "node_happy_2" [label=<<TABLE BORDER="0" CELLBORDER="0" CELLPADDING="6"><TR><TD ALIGN="left"><B>Happy Path #2</B></TD></TR><TR><TD ALIGN="left">System validates role permissions</TD></TR></TABLE>>];
  "node_happy_1" -> "node_happy_2";
    "node_happy_3" [label=<<TABLE BORDER="0" CELLBORDER="0" CELLPADDING="6"><TR><TD ALIGN="left"><B>Happy Path #3</B></TD></TR><TR><TD ALIGN="left">Request approved via email confirmation</TD></TR></TABLE>>];
  "node_happy_2" -> "node_happy_3";
    "node_happy_4" [label=<<TABLE BORDER="0" CELLBORDER="0" CELLPADDING="6"><TR><TD ALIGN="left"><B>Happy Path #4</B></TD></TR><TR><TD ALIGN="left">Access provisioned within 1 hour</TD></TR></TABLE>>];
  "node_happy_3" -> "node_happy_4";
    "node_happy_5" [label=<<TABLE BORDER="0" CELLBORDER="0" CELLPADDING="6"><TR><TD ALIGN="left"><B>Happy Path #5</B></TD></TR><TR><TD ALIGN="left">User receives login notification</TD></TR></TABLE>>];
  "node_happy_4" -> "node_happy_5";
  }
  subgraph cluster_unhappy {
    style=rounded;
    color="#B0BEC5";
    penwidth=1.0;
    fontname=Helvetica; fontsize=12; label="Exception Path";
    "node_unhappy_1" [label=<<TABLE BORDER="0" CELLBORDER="0" CELLPADDING="6"><TR><TD ALIGN="left"><B>Exception Path #1</B></TD></TR><TR><TD ALIGN="left">Request submitted by unauthorized user</TD></TR></TABLE>>];
  "node_start" -> "node_unhappy_1";
    "node_unhappy_2" [label=<<TABLE BORDER="0" CELLBORDER="0" CELLPADDING="6"><TR><TD ALIGN="left"><B>Exception Path #2</B></TD></TR><TR><TD ALIGN="left">Approval process fails due to missing digital signature</TD></TR></TABLE>>];
  "node_unhappy_1" -> "node_unhappy_2";
    "node_unhappy_3" [label=<<TABLE BORDER="0" CELLBORDER="0" CELLPADDING="6"><TR><TD ALIGN="left"><B>Exception Path #3</B></TD></TR><TR><TD ALIGN="left">Access not granted within SLA, requiring helpdesk escalation</TD></TR></TABLE>>];
  "node_unhappy_2" -> "node_unhappy_3";
  }
  "node_happy_5" -> "node_end";
  "node_unhappy_3" -> "node_end";
  "diagram_title" [shape=plaintext, label=<<TABLE BORDER="0" CELLBORDER="0" CELLPADDING="6"><TR><TD ALIGN="left"><B>Process</B></TD></TR><TR><TD ALIGN="left">Access Request &amp; Approval Â· AS-IS Process Flows</TD></TR></TABLE>>];
  "diagram_title" -> "node_start" [style=invis];
}